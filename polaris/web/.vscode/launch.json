{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "polaris-web",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run-script",
                "dev"
            ],
            "console": "internalConsole",
            "env": {
                "ENV": "development",
                "config": "./config.debug.js"
            }
        },
    ]
}